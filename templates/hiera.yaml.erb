---
<% unless @backends.empty? -%>
:backends:
<%= @backends.keys.to_yaml.split("\n")[1..-1].join("\n") %>
<% @backends.each_pair do |backend,data| -%>
:<%= backend %>:
<% data.each_pair do |key,value| -%>
  :<%= key %>: <%= value.gsub(/_percent_/, '%') %>
<% end -%>
<% end -%>
<% unless @hierarchy.empty? -%>
:hierarchy:
<%= @hierarchy.to_yaml.split("\n")[1..-1].join("\n").gsub(/_percent_/, '%') %>
<% end -%>
<% unless @merge_behavior.empty? -%>
:merge_behavior: <%= @merge_behavior %>
<% end -%>
